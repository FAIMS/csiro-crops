      <appendCharacterString><![CDATA[ - ]]></appendCharacterString>
      <lookup>
        <term>{Wheat}
<description/></term>
        <term>{Barley}
<description/></term>
        <term>{Oats}
<description/></term>
        <term>{Tricicale}
<description/></term>
        <term>{Rye}
