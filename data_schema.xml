        <term>{Lentils}
<description/></term>
        <term>{Vetch}
<description/></term>
        <term>{Sunflower}
<description/></term>
        <term>{Sorghum}
<description/></term>
        <term>{Cotton}
<description/></term>
        <term>{Rice}
<description/></term>
