        <term>{Volunteer_pasture}
<description/></term>
        <term>{Other}
<description/></term>
      </lookup>
    </property>
    <property name="Year" isIdentifier="true" type="measure">
      <description/>
      <formatString><![CDATA[{{if $1 then $1}}{{if and($1, $2) then " " }}{{if $2 then $2}}{{if $3 then " ($3)"}}{{if between($4,0,0.49) then "??" elsif lessThan($4,1) then "?" }}]]></formatString>
      <appendCharacterString><![CDATA[ - ]]></appendCharacterString>
    </property>
    <property name="Month of Sowing" type="measure">
      <description/>
    </property>
    <property name="Month of Harvest" type="vocab">
      <description/>
      <lookup>
        <term>{March}
<description/></term>
        <term>{April}
<description/></term>
        <term>{May}
<description/></term>
      </lookup>
    </property>
    <property name="Enterprise" type="vocab">
      <description/>
      <lookup>
        <term>{Yes}
<description/></term>
        <term>{No}
<description/></term>
      </lookup>
    </property>
    <property name="Photo" type="file" file="true" thumbnail="true">
